<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>PolicyholderTEL_13judgment</fullName>
    <active>true</active>
    <description>契約者TELが13文字であるか判定します。</description>
    <errorConditionFormula>AND(Not(IsBlank(PolicyholderTEL__c)) ,NOT(LEN(PolicyholderTEL__c) =13))</errorConditionFormula>
    <errorDisplayField>PolicyholderTEL__c</errorDisplayField>
    <errorMessage>契約者TELは、13文字です。</errorMessage>
</ValidationRule>
